{
  "title": "Ontology Viewer",
  "backendServices": {
    "db": "http://deep-cell.ucsd.edu:9200",
    "simulator": "http://deep-cell.ucsd.edu:8888"
  },

  "defaultNetwork": "go",

  "trees": {
    "go": {
      "name": "FanGO",
      "rootNode": "CLIXO:141",
      "searchOptions": {
        "index": "terms",
        "type": "go_term"
      },
      "properties": {
        "namespace": {
          "biological_process": {
            "tag": "BP",
            "color": "rgb(0,153,204)"
          },
          "cellular_component": {
            "tag": "CC",
            "color": "rgb(255,102,0)"
          },
          "molecular_function": {
            "tag": "MF",
            "color": "rgb(0,204,153)"
          }
        }
      }
    }
  }
}
